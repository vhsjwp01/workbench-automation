%
% config.desc : 
%
%
root-skeleton = 		"../trf-jcl/"
target-proc   =         "../trf-jcl/Master-Proc"
use-sort=mf-sort
%


%
var-dataroot = "${DATA}/"
var-tmp = "${TMP}/"
var-spool = "${SPOOL}/"

%
% Entete de Ksh ---------------------------------------------------------------------------------------------
%

top-skeleton = "../param/top-ksh.txt"

% Entete de Ksh ---------------------------------------------------------------------------------------------
%
% 

%
% Queue de KSH  ---------------------------------------------------------------------------------------------
%
bottom-skeleton = "../param/bottom-ksh.txt"
%
% Queue de KSH  ---------------------------------------------------------------------------------------------
%

%
% Fichier pass� en table 
%
file-list-in-table = "../param/dynamic-config/File-in-table.txt"
%

% Suffixe du ksh traduit
suffix-skeleton = "ksh"

% Gestion des FSN a conserver
set-no-delete-fsn = SORTIE ( ZIP390 ),
                    ENTREE ( ZIP390 ),
                    * ( DB2CMD,CSQUTIL ),
                    CFTIN ( CFTUTIL ),
                    SYSUT1 ( DUMMY ),
                    * ( ADRDSSU ).

% Gestion des FSN � supprimer
set-delete-fsn = SYSOUT ( IDCAMS ),
                 SYSEXEC ( CSCOLMVS ),
                 SYSPUNCH ( * ),
                 TOOLIN ( * ),
                 SYSUDUMP ( * ),
                 SYSDBOUT ( * ),
                 SYSABOUT ( * ),
                 SYSTSPRT ( * ),
                 SORTLIB ( * ),
                 OPLIB ( * ),
                 STEPLIB ( * ),
                 JOBLIB ( * ).
